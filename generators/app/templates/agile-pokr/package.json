{
  "name": "<%= appName %>",
  "version": "<%= appVersion %>",
  "id": "<%= appPackage %>",
  "description": "<%= appDescription %>",
  "author": "<%= appUserName %>",
  "homepage": "https://fidj.ovh/_/<%= appName %>",
  "main": "www/index.html",
  "scripts": {
    "test": "./node_modules/.bin/karma start tests/karma.unit-tests.conf.js",
    "clean": "ionic-app-scripts clean",
    "build:app": "ionic-app-scripts build",
    "build:config": "./node_modules/.bin/gulp",
    "build:ids": "./node_modules/.bin/gulp www-appId",
    "build": "npm run build:config && npm run build:app && npm run build:ids",
    "lint": "ionic-app-scripts lint",
    "serve": "ionic-app-scripts serve",
    "resources": "fidj-resources --ios --android --marketing && cp resources/ios/icon/icon-60.png src/assets/icon/favicon.ico",
    "ios": "npm run build && npm run resources && ionic cordova platform add ios && ionic cordova build ios"
  },
  "devDependencies": {
    "@ionic/app-scripts": "3.1.6",
    "gulp": "^3.9.1",
    "gulp-cheerio": "^0.6.3",
    "gulp-replace": "^0.6.1",
    "gulp-util": "^3.0.8",
    "jasmine-ajax": "^3.3.1",
    "jasmine-core": "^2.99.1",
    "karma": "^2.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.0",
    "karma-html-reporter": "*",
    "karma-jasmine": "^1.1.1",
    "karma-jasmine-ajax": "^0.1.13",
    "karma-jasmine-html-reporter-livereload": "*",
    "shelljs": "^0.8.1",
    "typescript": "2.4.2"
  },
  "dependencies": {
    "@angular/animations": "5.0.3",
    "@angular/common": "5.0.3",
    "@angular/compiler": "5.0.3",
    "@angular/compiler-cli": "5.0.3",
    "@angular/core": "5.0.3",
    "@angular/forms": "5.0.3",
    "@angular/http": "5.0.3",
    "@angular/platform-browser": "5.0.3",
    "@angular/platform-browser-dynamic": "5.0.3",
    "@ionic-native/core": "4.4.0",
    "@ionic-native/network": "^4.7.0",
    "@ionic-native/splash-screen": "4.4.0",
    "@ionic-native/status-bar": "4.4.0",
    "@ionic/storage": "2.1.3",
    "cordova-android": "^7.0.0",
    "cordova-ios": "4.5.4",
    "cordova-plugin-device": "^1.1.7",
    "cordova-plugin-ionic-webview": "^1.2.0",
    "cordova-plugin-network-information": "^2.0.1",
    "cordova-plugin-splashscreen": "^4.1.0",
    "cordova-plugin-sqlite-2": "^1.0.4",
    "cordova-plugin-statusbar": "^2.4.1",
    "cordova-plugin-whitelist": "^1.3.3",
    "ionic-angular": "3.9.2",
    "ionic-plugin-keyboard": "^2.2.1",
    "ionicons": "3.0.0",
    "fidj": "^2.1.7",
    "rxjs": "5.5.2",
    "save-dev": "^2.0.0",
    "sw-toolbox": "3.6.0",
    "zone.js": "0.8.18"
  },
  "cordova": {
    "platforms": [
      "android",
      "ios"
    ],
    "plugins": {
      "ionic-plugin-keyboard": {},
      "cordova-plugin-whitelist": {},
      "cordova-plugin-device": {},
      "cordova-plugin-splashscreen": {},
      "cordova-plugin-ionic-webview": {},
      "cordova-plugin-sqlite-2": {},
      "cordova-plugin-statusbar": {},
      "cordova-plugin-network-information": {}
    }
  }
}
